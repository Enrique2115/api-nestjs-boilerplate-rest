{
  "name": "api-rest-boilerplate-rest",
  "version": "0.0.1",
  "engines": {
    "node": ">=22.x",
    "pnpm": ">=10.x"
  },
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "nest start",
    "dev": "nest start --watch --debug=0.0.0.0:9229",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "rimraf coverage .nyc_output && concurrently 'npm:test:unit' 'npm:test:e2e' && npm run calculate-global-test-coverage",
    "test:unit": "vitest run --coverage --config ./vitest.config.unit.ts",
    "test:e2e": "vitest run --coverage --config ./vitest.config.e2e.ts",
    "calculate-global-test-coverage": "tsx scripts/calculate-global-test-coverage.ts",
    "prebuild": "rimraf dist; exit 0",
    "build": "nest build --path tsconfig.build.json",
    "clean": "git clean -xdf .nyc_output coverage dist node_modules documentation",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "prepare": "husky || true",
    "docss": "compodoc -p tsconfig.json -s"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/static": "^8.2.0",
    "@keyv/redis": "^4.4.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/platform-fastify": "^11.1.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "cache-manager": "^7.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.3",
    "joi": "^17.13.3",
    "nestjs-pino": "^4.4.0",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.24"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@compodoc/compodoc": "^1.1.26",
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.11.31",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitest/coverage-istanbul": "1.3.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-vitest": "^0.5.4",
    "fs-extra": "^11.3.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "nock": "^13.5.6",
    "nyc": "^17.1.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.3",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "unplugin-swc": "^1.5.3",
    "vite": "^6.3.5",
    "vitest": "1.3.1",
    "vitest-mock-extended": "^3.1.0"
  },
  "packageManager": "pnpm@10.11.0"
}
